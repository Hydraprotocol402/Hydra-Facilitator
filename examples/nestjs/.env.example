# EVM Configuration
# Private key for EVM network operations (hex string)
# At least one of EVM_PRIVATE_KEY or SVM_PRIVATE_KEY must be set
EVM_PRIVATE_KEY=

# Solana (SVM) Configuration
# Private key for Solana network operations (base58 encoded string)
# At least one of EVM_PRIVATE_KEY or SVM_PRIVATE_KEY must be set
SVM_PRIVATE_KEY=

# Optional: Custom Solana RPC URL
# If not provided, defaults to public Solana RPC endpoints based on network
SVM_RPC_URL=

# Server Port (default: 3000)
PORT=3000
